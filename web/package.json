        "@testing-library/react": "^14.3.1",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@vitest/coverage-v8": "^1.6.1",
        "autoprefixer": "^10.4.0",
        "jsdom": "^21.1.0",
        "playwright": "^1.40.0",
        "postcss": "^8.4.0",
        "prettier": "^3.6.2",
